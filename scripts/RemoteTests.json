[
    {
        "TestName": "Throughput",
        "Remote": {
            "Platform": "Windows",
            "Tls": ["stub", "schannel", "mitls", "openssl"],
            "Arch": ["x64", "x86", "arm", "arm64"],
            "Exe": "quicperf",
            "Arguments": {
                "All": "",
                "Loopback": "-selfsign:1",
                "Remote": "-thumbprint:$Thumbprint -machine_cert:1 -cert_store:My"
            }
        },
