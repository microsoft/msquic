            }
        },
        "Local" : {
            "Platform": "linux",
            "Tls": ["stub", "openssl"],
            "Arch": ["x64", "arm"],
            "Exe": "quicperf",
            "Arguments": {
                "All": "-TestName:Throughput -target:$RemoteAddress -port:4433 -uni:1 -length:2000000000",
                "Loopback": "",
                "Remote": ""
            }
        },
        "Variables": [
            {
                "Name": "Encryption",
                "Local": {
                    "On": "",
                    "Off": "-encrypt:0"
                },
                "Remote": {
                    "On": "",
                    "Off": "-encrypt:0"
                },
                "Default": "On"
            },
            {
                "Name": "SendBuffering",
                "Local": {
                    "On": "",
                    "Off": "-sendbuf:0"
                },
                "Remote": {
                    "On": "",
                    "Off": "-sendbuf:0"
                },
                "Default": "Off"
            }
        ],
        "AllowLoopback": true,
        "Iterations": 10,
        "RemoteReadyMatcher": "Started!",
        "ResultsMatcher": "Closed.*\\(TX.*bytes @ (.*) kbps\\)",
        "Units": "kbps"
    },
    {
        "TestName": "RPS",
        "Remote": {
            "Platform": "Windows",
            "Tls": ["stub", "schannel", "mitls"],
            "Arch": ["x64", "x86", "arm", "arm64"],
            "Exe": "quicperf",
            "Arguments": {
                "All": "-TestName:RPS -ServerMode:1-iter:10",
                "Loopback": "-selfsign:1",
                "Remote": "-thumbprint:$Thumbprint -machine_cert:1 -cert_store:My"
            }
        },
        "Local": {
            "Platform": "Windows",
            "Tls": ["stub", "schannel", "mitls"],
            "Arch": ["x64", "x86", "arm", "arm64"],
            "Exe": "quicperf",
            "Arguments": {
                "All": "-TestName:RPS -target:$RemoteAddress",
                "Loopback": "",
                "Remote": ""
            }
        },
        "Variables": [
            {
                "Name": "ConnectionCount",
                "Local": {
                    "100": "-conns:100",
                    "1000": "-conns:1000",
                    "10000": "-conns:10000"
                },
                "Remote": {
                    "100": "",
                    "1000": "",
                    "10000": ""
                },
                "Default": "1000"
            },
            {
                "Name": "RequestSize",
                "Local": {
                    "0": "-request:0",
                    "512": "-request:512",
                    "4096": "-request:4096",
                    "65536": "-request:65536"
                },
                "Remote": {
                    "0": "",
                    "512": "",
                    "4096": "",
                    "65536": ""
                },

                "Default": "0"
            },
            {
                "Name": "ResponseSize",
                "Local": {
                    "512": "",
                    "4096": "",
                    "65536": ""
                },
                "Remote": {
                    "512": "-response:512",
                    "4096": "-response:4096",
                    "65536": "-response:65536"
                },
                "Default": "512"
            }
        ],
        "AllowLoopback": false,
        "Iterations": 10,
        "RemoteReadyMatcher": "Started!",
        "ResultsMatcher": "Result: (.*) RPS",
        "Units": "RPS"
    }
]
